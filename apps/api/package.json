  "scripts": {
    "build": "prisma generate && tsc --build --verbose",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prisma:generate": "prisma format && prisma generate",
    "sync-db-schema": "prisma format && prisma generate &&node -r ts-node/register --env-file=.env scripts/sync-db-schema.ts",
